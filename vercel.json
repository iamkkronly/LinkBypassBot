{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bypass",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
